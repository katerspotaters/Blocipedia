<h1>Users</h1>

<div class = "tall teal scrollable container">

  <% @users.each do |user| %>
  <div class = 'row'>

    <div class="col-md-2 shift-down">
      <%= link_to user.name, user %>
    </div>

    <div class="col-md-2 col-md-offset-3 shift-down">
      <%= user.role %> user
    </div>

  </div>

  <% end %>

</div>
